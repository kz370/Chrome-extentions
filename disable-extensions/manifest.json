{
    "manifest_version": 3,
    "name": "Disable Extension on Specific Sites",
    "version": "1.0",
    "description": "Disables specific extensions on a list of specific sites.",
    "permissions": [
      "tabs",
      "storage",
      "management",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    }
  }
  